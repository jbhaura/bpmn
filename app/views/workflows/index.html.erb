<p style="color: green"><%= notice %></p>

<h1>Workflows</h1>

<div id="workflows">
  <% @workflows.each do |workflow| %>
    <div style="border: 1px solid black">
        <%= render workflow %>
        <p>
        <%= link_to "Show this workflow", workflow %>
        </p>
    </div>
  <% end %>
</div>

<%= link_to "New workflow", new_workflow_path %>
